{
  "name": "api_rest_users_typeorm",
  "version": "1.0.0",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "drop": "psql -U neto -c \"DROP DATABASE db_testes\" ",
    "new": "psql -U neto -c \"CREATE DATABASE db_testes\" ",
    "generate": "typeorm-ts-node-commonjs migration:generate -d src/data-source src/migrations/newMigration",
    "runner": "typeorm-ts-node-commonjs migration:run -d src/data-source",
    "dev": "ts-node-dev src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.12",
    "@types/supertest": "^2.0.12",
    "sqlite3": "^5.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jest": "^29.3.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.11",
    "yup": "^0.32.11"
  }
}
